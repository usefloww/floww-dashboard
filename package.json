{
  "name": "floww-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "typecheck": "tsc -b --incremental",
    "build": "tsc -b && NODE_OPTIONS='--max-old-space-size=8192' vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node dist/server/server.js",
    "prepare": "husky",
    "test": "vitest",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "worker": "tsx server/jobs/worker.ts",
    "db:migrate": "tsx server/db/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx server/db/seed.ts",
    "db:seed:reset": "tsx server/db/seed.ts --reset",
    "sdk:build": "pnpm --filter floww build",
    "sdk:test": "pnpm --filter floww test",
    "sdk:typecheck": "pnpm --filter floww typecheck",
    "knip": "knip",
    "env:pull": "tsx scripts/env-sync.ts pull",
    "env:push": "tsx scripts/env-sync.ts push"
  },
